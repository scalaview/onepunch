<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
          Apks
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                  <th>Name</th>
                  <th>Seller</th>
                  <th>Size</th>
                  <th>DownloadTime</th>
                  <th>is Active</th>
                  <th>Action</th>
              </tr>
            </thead>
            <tbody>
              {{#each apks}}
                <tr>
                  <td>{{ name }}</td>
                  <td>{{ seller.name }}</td>
                  <td>{{ sizeFormat apkSize }}</td>
                  <td>{{ downloadTime }}</td>
                  <td>{{ isActive }}</td>
                  <td>
                    <a href="#" class="list-group-item">
                      <i class="glyphicon glyphicon-pencil"></i>
                    </a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <!-- /.table-responsive -->
      </div>
      <!-- /.panel-body -->
    </div>
      <!-- /.panel -->
  </div>
</div>